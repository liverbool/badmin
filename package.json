{
    "name": "badmin",
    "description": "Admin panel written using backbone.js, marionette.js, bootstrap and gulp. Demo: http://mmukhin.github.io/badmin",
    "author": {
        "name": "Michael Mukhin",
        "email": "hello@psitsmike.com",
        "twitter": "@michaelmukhin"
    },
    "homepage": "https://github.com/mmukhin/badmin",
    "repository": {
        "type": "git",
        "url": "https://github.com/mmukhin/badmin"
    },
    "browserify-shim": {
        "plugin": {
            "exports": "plugin",
            "depends": [
                "jquery:$"
            ]
        }
    },
    "browserify": {
        "transform": [
            "browserify-shim",
            "coffeeify",
            "hbsfy"
        ]
    },
    "devDependencies": {
        "browser-sync": "~1.3.6",
        "browserify": "~5.10.1",
        "browserify-shim": "~3.7.0",
        "coffeeify": "~0.6.0",
        "gulp": "^3.8.7",
        "gulp-changed": "^0.4.1",
        "gulp-concat": "^2.4.1",
        "gulp-imagemin": "^0.6.2",
        "gulp-notify": "^1.4.2",
        "gulp-replace": "~0.5.2",
        "gulp-sass": "~1.3.2",
        "gulp-sourcemaps": "^1.3.0",
        "gulp-util": "^3.0.0",
        "handlebars": "^1.3.0",
        "hbsfy": "~2.0.0",
        "minimist": "~1.1.0",
        "pretty-hrtime": "~0.2.1",
        "streamqueue": "",
        "require-dir": "^0.1.0",
        "vinyl-source-stream": "~0.1.1",
        "watchify": "~1.0.2"
    },
    "dependencies": {
        "backbone": "~1.1.2",
        "backbone.marionette": "~2.3.2",
        "jquery": "~2.1.3",
        "underscore": "~1.7.0"
    }
}